<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.what.spring.mapper.AvatarMapper">

    <select id="getAllDefaultAvatar" resultType="Avatar">
        select *
        from avatatr
        where upload_user_id is null;
    </select>

</mapper>